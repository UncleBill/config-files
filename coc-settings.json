{
  "javascript.validate.enable": true,
  "typescript.suggest.autoImports": true,
  "typescript.format.enabled": true,
  "javascript.suggest.autoImports": true,
  "suggest.noselect": true,
  "coc.preferences.formatOnSaveFiletypes": ["*"],
  "eslint.autoFixOnSave": true,
  "coc.preferences.formatOnType": true,
  "coc.preferences.formatOnTypeFiletypes": ["*"],
  "styled-components.tags": ["styled", "css", "sty", "createGlobalStyle"],
  "translator.engines": ["bing", "youdao", "haici"],
  "explorer.openAction.strategy": "previousBuffer",
  "explorer.contentWidthType": "win-width",
  "explorer.file.child.template": "[indent][icon | 1] [diagnosticError & 1][filename omitCenter 1][modified][readonly] [linkIcon & 1][link growRight 1]",
  "explorer.toggle": true,
  "typescript.preferences.includePackageJsonAutoImports": "on",
  "colors.filetypes": ["*"],
  "git.enableGutters": true
}
